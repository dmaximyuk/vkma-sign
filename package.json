{
  "name": "@dmaksimyk/vkma-sign",
  "version": "0.1.1",
  "description": "This package VKMA Sign!",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmaksimyk/vkma-sign.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "rand",
    "random",
    "number"
  ],
  "author": "dmaksimyk",
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "cross-env NODE_PATH=./dist nodemon",
    "start": "node ./dist/index.js",
    "prepublish": "node --experimental-vm-modules node_modules/jest/bin/jest.js && rimraf dist && tsc",
    "publish": "npm publish --access public"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "exec": "tsc && node -r tsconfig-paths/register dist/index.js"
  },
  "devDependencies": {
    "@types/intl": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cross-env": "latest",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.12.0",
    "typescript": "latest"
  },
  "dependencies": {
    "querystring": "^0.2.1"
  }
}
